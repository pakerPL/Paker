<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="paker.main.view.LogInDialogController">
   <children>
      <Pane prefHeight="180.0" prefWidth="300.0">
         <children>
            <Label fx:id="message" layoutX="101.0" layoutY="36.0" text="Podaj login i hasło" />
            <TextField fx:id="login" layoutX="58.0" layoutY="53.0" prefHeight="25.0" prefWidth="184.0" />
            <PasswordField fx:id="pass" layoutX="58.0" layoutY="78.0" prefHeight="25.0" prefWidth="184.0" />
            <Button fx:id="ok" layoutX="57.0" layoutY="110.0" mnemonicParsing="false" onAction="#handleOK" prefHeight="33.0" prefWidth="93.0" text="Zaloguj" />
            <Button fx:id="exit" layoutX="150.0" layoutY="110.0" mnemonicParsing="false" onAction="#handleExit" prefHeight="33.0" prefWidth="93.0" text="Wyjdź" />
         </children>
      </Pane>
   </children>
</AnchorPane>
